{
  "name": "eslint-config-5app",
  "version": "0.15.1",
  "description": "Javascript Style guide for 5app",
  "main": "index.js",
  "scripts": {
    "semantic-release": "semantic-release",
    "lint": "eslint ./ --config eslintrc.json",
    "spec": "mocha test/validate.js",
    "test": "npm run lint && npm run spec"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/5app/eslint-config-5app.git"
  },
  "files": [
    "eslintrc.json",
    "index.js"
  ],
  "keywords": [
    "5app",
    "style",
    "guide"
  ],
  "author": "Andrew Dodson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/5app/eslint-config-5app/issues"
  },
  "homepage": "https://github.com/5app/eslint-config-5app#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "chai": "^4.3.4",
    "eslint": "^7.29.0",
    "mocha": "^9.0.1",
    "semantic-release": "^18.0.0"
  },
  "peerDependencies": {
    "eslint": "^7.15.0"
  },
  "release": {
    "branches": ["main"],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "eslint-plugin-jsdoc": "^36.0.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.12.0"
  }
}
